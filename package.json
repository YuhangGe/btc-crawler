{
  "name": "btc-crawler",
  "version": "1.0.0",
  "description": "btc crawler",
  "main": "app/index.js",
  "scripts": {
    "debug": "node ./app/index.js",
    "dev": "pm2 kill && pm2 start ecosystem.config.js --only btc-crawler-dev --no-daemon",
    "start": "pm2 start ecosystem.config.js --only btc-crawler",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --fix ./app",
    "ss": "sslocal -s $SS_SERVER_HOST -p $SS_SERVER_PORT -l $SS_LOCAL_PORT -b $SS_LOCAL_ADDR -k $SS_PASSWORD -m $SS_METHOD -t $SS_TIMEOUT",
    "docker": "concurrently --raw \"npm run ss\" \"NODE_ENV=production npm run debug\""
  },
  "author": "Yuhang Ge",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "concurrently": "^3.5.1",
    "elasticsearch": "^14.1.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "nodemailer": "^4.6.0",
    "pako": "^1.0.6",
    "pm2": "^2.9.3",
    "pm2-logrotate": "^2.4.0",
    "socks-proxy-agent": "^3.0.1",
    "ws": "^4.0.0"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "eslint": "^4.17.0",
    "precommit-hook": "^3.0.0"
  }
}
